<h1>Verbs</h1>

<table>
  <tr>
    <th>Language</th>
    <th>Id</th>
    <th>Coding frame</th>
    <th>Verb form</th>
    <th>Original script</th>
    <th>Comment</th>
  </tr>

<% @verbs.each do |verb| %>
  <tr>
    <td><%= verb.language %></td>
    <td><%= verb.id %></td>
    <td><%= verb.coding_frame %></td>
    <td><%= verb.verb_form %></td>
    <td><%= verb.original_script %></td>
    <td><%= verb.comment %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Verb', new_verb_path %>
